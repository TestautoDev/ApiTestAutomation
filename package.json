{
  "name": "apitestautomation",
  "version": "2.0.0",
  "description": "Test api using mocha, chai and supertest",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install;",
    "start": "JUNIT_REPORT_PATH=test-result/result.xml JUNIT_REPORT_STACK=1 mocha --timeout 25000 --colors --reporter mocha-jenkins-reporter || true",
    "test": "mocha -r ts-node/register test/*.spec.ts --timeout 45000 --reporter mochawesome",
    "start:server": "node server/server.js",
    "start:test": "concurrently \"npm:start:server\" \"npm:test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TestautoDev/ApiTestAutomation.git"
  },
  "keywords": [
    "api",
    "test",
    "automation"
  ],
  "author": "Dipjyoti Metia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TestautoDev/ApiTestAutomation/issues"
  },
  "homepage": "https://github.com/TestautoDev/ApiTestAutomation#readme",
  "dependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/ip": "^1.1.0",
    "@types/json-server": "^0.14.2",
    "@types/lodash": "^4.14.149",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "await-delay": "1.0.0",
    "axios": "0.19.2",
    "chai": "^4.2.0",
    "cryptr": "^6.0.1",
    "date-fns": "^2.11.0",
    "dotenv": "^8.2.0",
    "faker": "4.1.0",
    "ip": "^1.1.5",
    "json-server": "0.16.1",
    "knex": "^0.20.13",
    "lodash": "4.17.15",
    "lokijs": "1.5.8",
    "mocha": "^7.1.1",
    "mochawesome": "5.0.0",
    "moment": "2.24.0",
    "mssql": "6.2.0",
    "password-hash": "^1.2.2",
    "supertest": "^4.0.2",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@types/chai": "^4.2.11",
    "@types/expect": "^24.3.0",
    "@types/faker": "^4.1.10",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.3",
    "chalk": "3.0.0",
    "cross-env": "7.0.2",
    "npm-run-all": "^4.1.5",
    "prettyjson": "1.2.1",
    "rimraf": "^3.0.2",
    "sinon": "9.0.0",
    "ts-node": "^8.8.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "winston": "3.2.1"
  }
}
